@model IEnumerable<Book>

<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>Bi-Biblioteket</title>
</head>
<body>
    @foreach (var book in Model)
    {
        <div>
            <h2>@book.BookName</h2>
            <h3>@book.Author</h3>
            <h3><a asp-controller="Book" asp-action="Details" asp-route-id="@book.BookID">@book.BookName</a></h3>
        </div>
    }
</body>
</html>